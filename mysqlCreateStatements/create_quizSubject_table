CREATE TABLE `Asrcoo`.`quizSubject` (
  `quizSubject_id` INT NOT NULL AUTO_INCREMENT,
  `quiz_id` INT NOT NULL,
  `subject_id` INT NOT NULL,
  PRIMARY KEY (`quizSubject_id`),
  UNIQUE INDEX `quizSubject_id_UNIQUE` (`quizSubject_id` ASC) VISIBLE,
  INDEX `fk_quizSubject_subject_idx` (`subject_id` ASC) VISIBLE,
  INDEX `fk_quizSubject_quiz_idx` (`quiz_id` ASC) VISIBLE,
  CONSTRAINT `fk_quizSubject_subject`
    FOREIGN KEY (`subject_id`)
    REFERENCES `Asrcoo`.`subject` (`subject_id`)
    ON DELETE NO ACTION
    ON UPDATE NO ACTION,
  CONSTRAINT `fk_quizSubject_quiz`
    FOREIGN KEY (`quiz_id`)
    REFERENCES `Asrcoo`.`quiz` (`quiz_id`)
    ON DELETE NO ACTION
    ON UPDATE NO ACTION);


